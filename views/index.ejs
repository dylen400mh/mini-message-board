<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel="stylesheet" href="/stylesheets/style.css" />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>
    <ul>
      <% messages.forEach(message => { %>
      <li>
        Message: <%= message.text %>, User: <%= message.user %>, Date: <%=
        message.added %>
      </li>
      <% }); %>
    </ul>
  </body>
</html>
